{
  "name": "bluemvc/bluemvc-core",
  "description": "Core functionality for the BlueMvc PHP framework",
  "homepage": "https://bluemvc.com/",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "blue",
    "mvc",
    "framework",
    "php",
    "core"
  ],
  "authors": [
    {
      "name": "Michael Hall",
      "homepage": "https://michaelhall.se/"
    }
  ],
  "require": {
    "php": ">=7.1",
    "datatypes/datatypes": "^1.0||^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "BlueMvc\\Core\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BlueMvc\\Core\\Tests\\": "tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-develop": "2.1-dev"
    }
  }
}
